{
  "name": "nfl-pool-fe",
  "version": "0.0.2",
  "description": "NFL confidence pool, front-end only.",
  "keywords": [
    "aswnn",
    "nextjs",
    "typescript",
    "NFL",
    "graphql"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-pool-fe.git"
  },
  "scripts": {
    "build": "npm run clean && next build",
    "clean": "rm -rf .next/cache",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record",
    "dev": "next dev --port 3000 --turbo",
    "dev:old": "next dev --port 3000",
    "env": "sync-dotenv --env .env.local",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "license": "license-check-and-add add -f license-check-and-add-config.json",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "lint:style": "stylelint .",
    "lint:style:fix": "stylelint . --fix",
    "prepare": "husky install",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter' --runInBand",
    "test:watch": "jest --watch --maxWorkers=25%",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune -e"
  },
  "config": {},
  "dependencies": {
    "@bduff9/pro-duotone-svg-icons": "5.15.4",
    "@bduff9/pro-solid-svg-icons": "5.15.4",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "@happykit/analytics": "1.0.1",
    "@happykit/flags": "3.0.0",
    "@hookform/resolvers": "2.9.10",
    "@next-auth/prisma-adapter": "1.0.5",
    "@next/font": "13.1.1",
    "@prisma/client": "4.8.0",
    "bootstrap": "5.2.3",
    "clsx": "1.2.1",
    "fuse.js": "6.6.2",
    "js-cookie": "3.0.1",
    "logrocket": "3.0.1",
    "mysql2": "2.3.3",
    "next": "13.1.1",
    "next-auth": "4.18.7",
    "next-pwa": "5.6.0",
    "nodemailer": "6.8.0",
    "nprogress": "0.2.0",
    "pino": "8.8.0",
    "pino-logflare": "0.3.12",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-bootstrap": "2.7.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.3",
    "react-infinite-scroll-hook": "4.0.4",
    "react-loading-skeleton": "3.1.0",
    "react-quill": "2.0.0",
    "react-toastify": "9.1.1",
    "recharts": "2.2.0",
    "swr": "2.0.0",
    "throttle-debounce": "5.0.0",
    "workbox-window": "6.5.4",
    "yup": "0.32.11",
    "yup-phone": "1.3.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/bootstrap": "5.2.6",
    "@types/draft-js": "0.11.10",
    "@types/eslint": "8.4.10",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "29.2.5",
    "@types/js-cookie": "3.0.2",
    "@types/logrocket-react": "3.0.0",
    "@types/next-auth": "3.15.0",
    "@types/node": "18.11.18",
    "@types/nodemailer": "6.4.7",
    "@types/nprogress": "0.2.0",
    "@types/pino": "7.0.5",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.26",
    "@types/react-beautiful-dnd": "13.1.3",
    "@types/react-dom": "18.0.10",
    "@types/sass": "1.43.1",
    "@types/stylelint": "14.0.0",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/throttle-debounce": "5.0.0",
    "@types/yup": "0.32.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@wixc3/react-board": "^2.1.3",
    "autoprefixer": "10.4.13",
    "cypress": "12.2.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-clean-regex": "0.5.2",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-css-modules": "2.1.0",
    "license-check-and-add": "4.0.5",
    "lint-staged": "13.1.0",
    "npm-package-json-lint": "6.4.0",
    "pino-pretty": "9.1.1",
    "postcss": "8.4.20",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.8.3",
    "prettier": "2.8.1",
    "prisma": "4.8.0",
    "sass": "1.57.1",
    "scriptlint": "2.2.0",
    "stylelint": "14.16.1",
    "stylelint-config-twbs-bootstrap": "7.0.0",
    "sync-dotenv": "2.7.0",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4",
    "vercel": "28.10.1"
  },
  "engines": {
    "node": "18.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{md,json,css,scss}": [
      "prettier --write"
    ],
    "package.json": [
      "scriptlint --fix --config .stylelintrc.json",
      "npmPkgJsonLint ."
    ],
    "*.js": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
