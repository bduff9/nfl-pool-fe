{
  "name": "nfl-pool-fe",
  "version": "0.0.2",
  "description": "NFL confidence pool, front-end only.",
  "keywords": [
    "aswnn",
    "nextjs",
    "typescript",
    "NFL",
    "graphql"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-pool-fe.git"
  },
  "scripts": {
    "build": "npm run clean && next build",
    "clean": "rm -rf .next/cache",
    "cypress:open": "cypress open",
    "cypress:percy": "percy exec -- cypress run --record",
    "cypress:run": "cypress run --record",
    "dev": "next dev --port 3000",
    "env": "sync-dotenv",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "license": "license-check-and-add add -f license-check-and-add-config.json",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "lint:style": "stylelint .",
    "lint:style:fix": "stylelint . --fix",
    "prepare": "husky install",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter' --runInBand",
    "test:watch": "jest --watch --maxWorkers=25%",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune"
  },
  "config": {},
  "dependencies": {
    "@bduff9/pro-duotone-svg-icons": "5.15.3",
    "@bduff9/pro-solid-svg-icons": "5.15.3",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@hookform/resolvers": "2.6.0",
    "@sentry/nextjs": "6.8.0",
    "axios": "0.21.1",
    "bootstrap": "5.0.2",
    "clsx": "1.1.1",
    "framer-motion": "4.1.17",
    "fuse.js": "6.4.6",
    "graphql": "15.5.1",
    "graphql-request": "3.4.0",
    "js-cookie": "2.2.1",
    "logrocket": "1.3.0",
    "logrocket-react": "4.0.1",
    "mysql": "2.18.1",
    "next": "11.0.1",
    "next-auth": "<3.18.0",
    "next-pwa": "5.2.23",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-bootstrap": "next",
    "react-dom": "17.0.2",
    "react-hook-form": "7.10.1",
    "react-loading-skeleton": "2.2.0",
    "recharts": "2.0.9",
    "swr": "0.5.6",
    "swr-sync-storage": "1.0.1",
    "throttle-debounce": "3.0.1",
    "tslog": "3.2.0",
    "yup": "0.32.9",
    "yup-phone": "1.2.19"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-operations": "1.18.3",
    "@percy/cli": "1.0.0-beta.58",
    "@percy/cypress": "3.1.0",
    "@types/bootstrap": "5.0.16",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.2.13",
    "@types/jest": "26.0.23",
    "@types/js-cookie": "2.2.6",
    "@types/logrocket-react": "3.0.0",
    "@types/mysql": "2.15.18",
    "@types/next-auth": "3.15.0",
    "@types/node": "16.0.0",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.13",
    "@types/react-dom": "17.0.9",
    "@types/sass": "1.16.0",
    "@types/stylelint": "13.13.1",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/throttle-debounce": "2.1.0",
    "@types/yup": "0.29.12",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "@welldone-software/why-did-you-render": "6.2.0",
    "autoprefixer": "10.2.6",
    "cypress": "7.6.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-clean-regex": "0.5.1",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettierx": "0.18.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "7.0.0",
    "jest": "27.0.6",
    "jest-css-modules": "2.1.0",
    "license-check-and-add": "4.0.2",
    "lint-staged": "11.0.0",
    "npm-package-json-lint": "5.1.0",
    "postcss": "8.3.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "sass": "1.35.1",
    "scriptlint": "2.1.6",
    "stylelint": "13.13.1",
    "stylelint-config-twbs-bootstrap": "2.2.1",
    "sync-dotenv": "2.6.2",
    "terser-webpack-plugin": "5.1.4",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5",
    "vercel": "23.0.1"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "git add"
    ],
    "*.{md,json,css,scss}": [
      "prettierx --write",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix --config .stylelintrc.json",
      "npmPkgJsonLint .",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
