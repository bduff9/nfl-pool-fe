{
  "name": "nfl-pool-fe",
  "version": "0.0.2",
  "description": "NFL confidence pool, front-end only.",
  "keywords": [
    "aswnn",
    "nextjs",
    "typescript",
    "NFL",
    "graphql"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-pool-fe.git"
  },
  "scripts": {
    "build": "npm run clean && next build",
    "clean": "rm -rf .next/cache",
    "cypress:open": "cypress open",
    "cypress:percy": "percy exec -- cypress run --record",
    "cypress:run": "cypress run --record",
    "dev": "next dev --port 3000",
    "env": "sync-dotenv",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "license": "license-check-and-add add -f license-check-and-add-config.json",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "lint:style": "stylelint .",
    "lint:style:fix": "stylelint . --fix",
    "prepare": "husky install",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter' --runInBand",
    "test:watch": "jest --watch --maxWorkers=25%",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune"
  },
  "config": {},
  "dependencies": {
    "@bduff9/pro-duotone-svg-icons": "5.15.4",
    "@bduff9/pro-solid-svg-icons": "5.15.4",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "@hookform/resolvers": "2.7.1",
    "@sentry/nextjs": "6.11.0",
    "axios": "0.21.1",
    "bootstrap": "5.1.0",
    "clsx": "1.1.1",
    "framer-motion": "4.1.17",
    "fuse.js": "6.4.6",
    "graphql": "15.5.1",
    "graphql-request": "3.5.0",
    "js-cookie": "3.0.0",
    "logrocket": "2.0.0",
    "logrocket-react": "4.0.1",
    "mysql2": "2.3.0",
    "next": "11.1.0",
    "next-auth": "<3.18.0",
    "next-pwa": "5.3.1",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-bootstrap": "next",
    "react-dom": "17.0.2",
    "react-hook-form": "7.12.2",
    "react-infinite-scroll-hook": "4.0.1",
    "react-json-view": "1.21.3",
    "react-loading-skeleton": "2.2.0",
    "recharts": "2.1.0",
    "swr": "0.5.6",
    "swr-sync-storage": "1.0.1",
    "throttle-debounce": "3.0.1",
    "tslog": "3.2.1",
    "yup": "0.32.9",
    "yup-phone": "1.2.19"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@percy/cli": "1.0.0-beta.65",
    "@percy/cypress": "3.1.1",
    "@types/bootstrap": "5.1.2",
    "@types/enzyme": "3.10.9",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.28.0",
    "@types/jest": "27.0.1",
    "@types/js-cookie": "2.2.7",
    "@types/logrocket-react": "3.0.0",
    "@types/next-auth": "3.15.0",
    "@types/node": "16.7.1",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.19",
    "@types/react-beautiful-dnd": "13.1.1",
    "@types/react-dom": "17.0.9",
    "@types/sass": "1.16.1",
    "@types/stylelint": "13.13.1",
    "@types/terser-webpack-plugin": "5.0.4",
    "@types/throttle-debounce": "2.1.0",
    "@types/yup": "0.29.13",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "@welldone-software/why-did-you-render": "6.2.0",
    "autoprefixer": "10.3.2",
    "cypress": "8.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-plugin-clean-regex": "0.5.1",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "<2.24.0",
    "eslint-plugin-prettierx": "0.18.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "jest-css-modules": "2.1.0",
    "license-check-and-add": "4.0.2",
    "lint-staged": "11.1.2",
    "npm-package-json-lint": "5.2.3",
    "postcss": "8.3.6",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "sass": "1.38.0",
    "scriptlint": "2.1.7",
    "stylelint": "13.13.1",
    "stylelint-config-twbs-bootstrap": "2.2.3",
    "sync-dotenv": "2.6.2",
    "terser-webpack-plugin": "5.1.4",
    "ts-jest": "27.0.5",
    "typescript": "4.4.2",
    "vercel": "23.1.2"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{md,json,css,scss}": [
      "prettierx --write"
    ],
    "package.json": [
      "scriptlint --fix --config .stylelintrc.json",
      "npmPkgJsonLint ."
    ],
    "*.js": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
