{
  "name": "nfl-pool-fe",
  "version": "0.0.2",
  "description": "NFL confidence pool, front-end only.",
  "keywords": [
    "aswnn",
    "nextjs",
    "typescript",
    "NFL",
    "graphql"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/nfl-pool-fe.git"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -r .next",
    "cypress:open": "cypress open",
    "cypress:percy": "percy exec -- cypress run --record",
    "cypress:run": "cypress run --record",
    "dev": "next dev",
    "env": "sync-dotenv",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "lint:style": "stylelint .",
    "lint:style:fix": "stylelint . --fix",
    "prepare": "husky install",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter' --runInBand",
    "test:watch": "jest --watch --maxWorkers=25%",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune"
  },
  "config": {},
  "dependencies": {
    "@bduff9/pro-duotone-svg-icons": "5.15.3",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@sentry/browser": "6.2.5",
    "axios": "0.21.1",
    "bootstrap": "5.0.0-beta3",
    "clsx": "1.1.1",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "js-cookie": "2.2.1",
    "jwt-simple": "0.5.6",
    "logrocket": "1.0.14",
    "logrocket-react": "4.0.1",
    "mysql": "2.18.1",
    "next": "10.1.3",
    "next-auth": "3.14.0",
    "next-pwa": "5.2.3",
    "nprogress": "0.2.0",
    "react": "17.0.2",
    "react-bootstrap": "next",
    "react-dom": "17.0.2",
    "swr": "0.5.5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@percy/cli": "1.0.0-beta.47",
    "@percy/cypress": "3.0.0",
    "@types/bootstrap": "5.0.12",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.2.8",
    "@types/jest": "26.0.22",
    "@types/js-cookie": "2.2.6",
    "@types/logrocket-react": "3.0.0",
    "@types/mysql": "2.15.18",
    "@types/next-auth": "3.13.0",
    "@types/node": "14.14.37",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/sass": "1.16.0",
    "@types/stylelint": "9.10.1",
    "@types/terser-webpack-plugin": "5.0.3",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "@welldone-software/why-did-you-render": "6.1.1",
    "autoprefixer": "10.2.5",
    "cypress": "7.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.23.0",
    "eslint-plugin-clean-regex": "0.5.1",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettierx": "0.17.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "npm-package-json-lint": "5.1.0",
    "postcss": "8.2.9",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0",
    "sass": "1.32.8",
    "scriptlint": "2.1.3",
    "stylelint": "13.12.0",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "21.0.0",
    "sync-dotenv": "2.6.2",
    "terser-webpack-plugin": "5.1.1",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4",
    "vercel": "21.3.3"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "git add"
    ],
    "*.{md,json,css,scss}": [
      "prettierx --write",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix --config .stylelintrc.json",
      "npmPkgJsonLint .",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.{css,scss}": "stylelint --fix"
  },
  "jest": {
    "moduleNameMapper": {},
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  }
}
